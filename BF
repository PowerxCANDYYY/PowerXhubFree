tab:Toggle("Auto-Set Spawn","Power x",false, function(t)
_G.AutoSetSpawn = t
while _G.AutoSetSpawn do wait()
   local args = {
	  [1] = "SetSpawnPoint"
   }
   game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer(unpack(args))
end
end)






